        "display_name": "Advanced System Monitor",
        "schema": {
          "type": "object",
          "properties": {
            "metrics": {
              "type": "array",
              "items": {"type": "string"}
            },
            "interval": {"type": "integer", "minimum": 1, "maximum": 3600},
            "alerting": {
              "type": "object",
              "properties": {
                "enabled": {"type": "boolean"},
                "thresholds": {"type": "object", "additionalProperties": {"type": "number"}},
                "notification_channels": {"type": "array", "items": {"type": "string"}}
              }
            }
          }
        },
        "annotations": {
          "category": "monitoring",
          "priority": "high",
          "version": "0.6.0",
          "features": "alerting,notifications"
        }
      },
      {
        "name": "security_scanner_v060",
