          "type": "object",
          "properties": {
            "target": {"type": "string", "description": "Target to scan"},
            "scan_type": {"type": "string", "enum": ["port", "vulnerability", "compliance", "full"]},
            "severity_filter": {"type": "array", "items": {"type": "string", "enum": ["low", "medium", "high", "critical"]}},
            "output_format": {"type": "string", "enum": ["json", "xml", "html", "pdf"], "default": "json"}
