{
    "version": 1,
    "disable_existing_loggers": "True",
    "formatters": {
        "json": {
            "()": "toolops.enrichment.python_tool_enrichment.enrichment_utils.tool.utils.utils.JsonFormatter"
        }
    },
    "handlers": {
        "console_stdout": {
            "class": "logging.StreamHandler",
            "formatter": "json",
            "level": "DEBUG"
        },
        "file_handler_stdout": {
            "formatter": "json",
            "level": "DEBUG",
            "class": "logging.FileHandler",
            "filename": "log/output.log",
            "mode": "a"
        }
    },
    "loggers": {
        "toolops": {
            "level": "DEBUG",
            "handlers": [
                "console_stdout",
                "file_handler_stdout"
            ],
            "propagate": true
        },
        "__main__": {
            "level": "DEBUG",
            "handlers": [
                "console_stdout",
                "file_handler_stdout"
            ],
            "propagate": false
        },
        "oawalker": {
            "level": "DEBUG",
            "handlers": [
                "console_stdout",
                "file_handler_stdout"
            ],
            "propagate": false
        },
        "*": {
            "level": "DEBUG",
            "handlers": [
                "console_stdout"
            ],
            "propagate": false
        },
        "root": {
            "level": "ERROR",
            "propagate": true
        }
    }
}